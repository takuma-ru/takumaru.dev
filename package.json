{
	"name": "takumaru.dev",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"deploy:pages": "npm run build && wrangler pages deploy ./build/client",
		"dev": "concurrently \"pnpm dev:remix\" \"pnpm dev:tsm\"",
		"dev:remix": "remix vite:dev",
		"dev:tsm": "typed-scss-modules app --watch",
		"lint:diff": "biome lint .",
		"lint:fix": "biome lint --apply .",
		"lint:style": "stylelint \"**/*.{css,scss,sass,html}\" --ignore-path .stylelintIgnore",
		"format:diff": "biome format .",
		"format:fix": "biome format --write .",
		"start": "wrangler pages dev ./build/client",
		"typecheck": "tsc",
		"typegen": "wrangler types",
		"preview": "npm run build && wrangler pages dev ./build/client",
		"build-cf-types": "wrangler types",
		"gen:component": "plop component --plopfile .plop/component/index.mjs",
		"gen:hook": "plop --plopfile .plop/hook/index.mjs",
		"gen:constant": "plop --plopfile .plop/constant/index.mjs",
		"sb:dev": "storybook dev -p 6006 --no-open",
		"sb:build": "storybook build"
	},
	"dependencies": {
		"@remix-run/cloudflare": "^2.8.1",
		"@remix-run/cloudflare-pages": "^2.8.1",
		"@remix-run/react": "^2.8.1",
		"clsx": "^2.1.0",
		"destyle.css": "^4.0.1",
		"isbot": "^4.1.0",
		"miniflare": "^3.20231030.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-markdown": "^9.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.6.4",
		"@chromatic-com/storybook": "^1.3.1",
		"@cloudflare/workers-types": "^4.20240405.0",
		"@iconify-json/material-symbols-light": "^1.1.18",
		"@remix-run/dev": "^2.8.1",
		"@remix-run/testing": "^2.8.1",
		"@storybook/addon-essentials": "^8.0.6",
		"@storybook/addon-interactions": "^8.0.6",
		"@storybook/addon-links": "^8.0.6",
		"@storybook/addon-onboarding": "^8.0.6",
		"@storybook/blocks": "^8.0.6",
		"@storybook/manager-api": "^8.0.8",
		"@storybook/react": "^8.0.6",
		"@storybook/react-vite": "^8.0.6",
		"@storybook/test": "^8.0.6",
		"@storybook/theming": "^8.0.8",
		"@svgr/core": "^8.1.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@takuma-ru/auto-story-generator": "^0.3.3",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"concurrently": "^8.2.2",
		"node-fetch": "^3.3.2",
		"plop": "^4.0.1",
		"sass": "^1.74.1",
		"storybook": "^8.0.6",
		"stylelint": "^16.3.1",
		"stylelint-config-recess-order": "^5.0.0",
		"stylelint-config-standard-scss": "^13.1.0",
		"typed-scss-modules": "^8.0.1",
		"typescript": "^5.1.6",
		"unplugin-icons": "^0.18.5",
		"vite": "^5.1.0",
		"vite-tsconfig-paths": "^4.2.1",
		"wrangler": "^3.24.0"
	},
	"engines": {
		"node": ">=20.12.1"
	},
	"packageManager": "pnpm@8.15.6",
	"volta": {
		"node": "20.12.1",
		"pnpm": "8.15.6"
	}
}
